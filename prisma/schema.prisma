generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Insured {
  id                   Int      @id @default(autoincrement())
  identificationType   Int
  identificationNumber String   @unique
  lastname             String
  name                 String
  gender               Int
  birthDate            DateTime
}

model Protections {
  id      Int       @id @default(autoincrement())
  name    String    @unique
  Premium Premium[]
}

model Premium {
  id           Int         @id @default(autoincrement())
  minAge       Int
  maxAge       Int
  percent      Float
  protectionId Int
  protection   Protections @relation(fields: [protectionId], references: [id])
}
